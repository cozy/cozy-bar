{
  "name": "cozy-bar",
  "version": "0.0.0-development",
  "description": "cozy-bar.js library, a small lib provided by cozy-stack to inject the Cozy-bar component into each app",
  "main": "dist/index.js",
  "author": "Cozy Cloud <contact@cozycloud.cc> (https://cozy.io/)",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cozy/cozy-bar.git"
  },
  "homepage": "https://github.com/cozy/cozy-bar",
  "bugs": {
    "url": "https://github.com/cozy/cozy-bar/issues"
  },
  "license": "MIT",
  "scripts": {
    "build": "env BABEL_ENV=transpilation babel ./src -d ./dist --extensions .js,.jsx --ignore '**/*.spec.jsx','**/*.spec.js' --copy-files",
    "prepublishOnly": "yarn build",
    "clean": "rm -rf ./dist/*",
    "lint": "eslint '{config,src,examples,test}/**/*.{js,jsx}'",
    "prebuild": "npm-run-all clean",
    "prewatch": "npm-run-all clean",
    "tx": "tx pull --all || true",
    "test": "jest",
    "travis-deploy-once": "travis-deploy-once",
    "example": "env USE_REACT=true nf -j examples/Procfile start",
    "start": "yarn build --watch"
  },
  "devDependencies": {
    "@babel/cli": "7.12.1",
    "@babel/core": "7.6.2",
    "@babel/polyfill": "^7.10.4",
    "@cozy/minilog": "^1.0.0",
    "@material-ui/core": "4.12.4",
    "@svgr/cli": "5.4.0",
    "@testing-library/react": "^10.2.1",
    "@testing-library/jest-dom": "^6.2.0",
    "babel-jest": "24.9.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-plugin-inline-json-import": "^0.3.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-define": "^2.0.0",
    "babel-preset-cozy-app": "^1.9.0",
    "cozy-authentication": "1.19.1",
    "cozy-client": "^40.5.0",
    "cozy-device-helper": "2.6.0",
    "cozy-flags": "^2.10.2",
    "cozy-intent": "^1.7.0",
    "cozy-interapp": "0.4.9",
    "cozy-logger": "1.9.1",
    "cozy-realtime": "^4.0.5",
    "cozy-ui": "^93.1.1",
    "cssnano-preset-advanced": "4.0.7",
    "deep-equal": "1.1.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.3",
    "enzyme-to-json": "3.3.5",
    "eslint-config-cozy-app": "2.0.0",
    "file-loader": "2.0.0",
    "foreman": "3.0.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "jest-fetch-mock": "^3.0.3",
    "json-loader": "0.5.7",
    "mini-css-extract-plugin": "0.8.0",
    "npm-run-all": "4.1.5",
    "parcel": "1.12.3",
    "react": "16.10.1",
    "react-dom": "16.10.1",
    "react-router": "3.2.4",
    "redux-logger": "3.0.6",
    "semantic-release": "15.13.24",
    "style-loader": "0.23.1",
    "stylus": "0.57.0",
    "stylus-loader": "3.0.2",
    "svg-sprite-loader": "4.1.6",
    "url-loader": "1.1.2"
  },
  "dependencies": {
    "hammerjs": "2.0.8",
    "lodash.debounce": "4.0.8",
    "lodash.set": "^4.3.2",
    "lodash.unionwith": "4.6.0",
    "prop-types": "15.7.2",
    "react-redux": "5.1.1",
    "redux": "4.1.2",
    "redux-persist": "5.10.0",
    "redux-thunk": "2.3.0",
    "semver-compare": "^1.0.0"
  },
  "peerDependencies": {
    "@cozy/minilog": "^1.0.0",
    "cozy-client": "^40.5.0",
    "cozy-device-helper": "2.6.0",
    "cozy-flags": "^2.10.2",
    "cozy-interapp": "0.4.9",
    "cozy-logger": "1.9.1",
    "cozy-realtime": "^4.0.5",
    "cozy-ui": "^93.1.1"
  }
}
