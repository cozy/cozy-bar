{
  "name": "cozy-bar",
  "version": "0.0.0-development",
  "description": "cozy-bar.js library, a small lib provided by cozy-stack to inject the Cozy-bar component into each app",
  "main": "dist/index.js",
  "author": "Cozy Cloud <contact@cozycloud.cc> (https://cozy.io/)",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cozy/cozy-bar.git"
  },
  "homepage": "https://github.com/cozy/cozy-bar",
  "bugs": {
    "url": "https://github.com/cozy/cozy-bar/issues"
  },
  "license": "MIT",
  "scripts": {
    "build": "env BABEL_ENV=transpilation babel ./src -d ./dist --extensions .js,.jsx --ignore '**/*.spec.jsx','**/*.spec.js' --copy-files",
    "prepublishOnly": "yarn build",
    "clean": "rm -rf ./dist/*",
    "lint": "yarn lint:js && yarn lint:styles",
    "lint:js": "eslint '{config,src,examples,test}/**/*.{js,jsx}'",
    "lint:styles": "stylint src --config ./node_modules/stylus-config-cozy-app/.stylintrc",
    "prebuild": "npm-run-all clean",
    "prewatch": "npm-run-all clean",
    "tx": "tx pull --all || true",
    "test": "jest",
    "start": "yarn build --watch"
  },
  "devDependencies": {
    "@babel/cli": "7.12.1",
    "@babel/helper-builder-binary-assignment-operator-visitor": "^7.25.9",
    "@babel/helper-builder-react-jsx": "^7.27.1",
    "@babel/helper-builder-react-jsx-experimental": "^7.12.11",
    "@babel/helper-define-map": "^7.18.6",
    "@babel/helper-hoist-variables": "^7.24.7",
    "@babel/helper-regex": "^7.10.5",
    "@babel/polyfill": "^7.10.4",
    "@cozy/minilog": "^1.0.0",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^10.2.1",
    "babel-jest": "24.9.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-inline-json-import": "^0.3.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-define": "^2.0.0",
    "babel-preset-cozy-app": "^2.8.1",
    "cozy-client": "^60.14.0",
    "cozy-dataproxy-lib": "^4.1.0",
    "cozy-device-helper": "2.6.0",
    "cozy-flags": "^4.6.1",
    "cozy-intent": "^2.29.1",
    "cozy-interapp": "0.4.9",
    "cozy-logger": "1.9.1",
    "cozy-realtime": "^4.0.5",
    "cozy-search": "^0.4.0",
    "cozy-ui": "^122.12.1",
    "cozy-ui-plus": "^2.1.0",
    "eslint-config-cozy-app": "2.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "npm-run-all": "4.1.5",
    "react": "16.10.1",
    "react-dom": "16.10.1",
    "react-router-dom": "6.14.2",
    "semantic-release": "15.13.24",
    "stylint": "^2.0.0",
    "stylus": "0.57.0",
    "stylus-config-cozy-app": "^0.1.0"
  },
  "dependencies": {
    "lodash.set": "^4.3.2",
    "lodash.unionwith": "4.6.0",
    "prop-types": "15.7.2",
    "react-redux": "5.1.1",
    "redux": "4.1.2",
    "redux-logger": "3.0.6",
    "redux-persist": "5.10.0",
    "redux-thunk": "2.3.0"
  },
  "peerDependencies": {
    "@cozy/minilog": ">=1.0.0",
    "cozy-client": ">=60.14.0",
    "cozy-dataproxy-lib": ">=4.1.0",
    "cozy-device-helper": ">=2.6.0",
    "cozy-flags": ">=4.6.1",
    "cozy-intent": ">=2.29.1",
    "cozy-interapp": ">=0.4.9",
    "cozy-realtime": ">=4.0.5",
    "cozy-search": ">=0.4.0",
    "cozy-ui": ">=122.12.1",
    "cozy-ui-plus": ">=2.1.0",
    "react": ">=16.10.1",
    "react-dom": ">=16.10.1",
    "react-router-dom": ">=6.14.2"
  }
}
