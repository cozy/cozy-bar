{
  "name": "cozy-bar",
  "version": "0.0.0-development",
  "description": "cozy-bar.js library, a small lib provided by cozy-stack to inject the Cozy-bar component into each app",
  "main": "dist/cozy-bar.js",
  "author": "Cozy Cloud <contact@cozycloud.cc> (https://cozy.io/)",
  "contributors": [
    "m4dz (https://m4dz.net)",
    "CPatchane"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cozy/cozy-bar.git"
  },
  "homepage": "https://github.com/cozy/cozy-bar",
  "bugs": {
    "url": "https://github.com/cozy/cozy-bar/issues"
  },
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "npm-run-all --parallel 'build:*' 'build:*:*'",
    "build:browser": "webpack --config ./config/webpack.target.browser.js",
    "build:browser:min": "NODE_ENV=production webpack --config ./config/webpack.target.browser.js",
    "build:mobile": "webpack --config ./config/webpack.target.mobile.js",
    "build:mobile:min": "NODE_ENV=production webpack --config ./config/webpack.target.mobile.js",
    "clean": "rm -rf ./dist/*",
    "lint": "eslint 'src/**/*.{js,jsx}'",
    "prebuild": "npm-run-all lint clean tx",
    "prewatch": "npm-run-all clean",
    "tx": "tx pull --all || true",
    "watch": "NODE_ENV=development webpack --config ./config/webpack.target.browser.js --watch --display-chunks",
    "watch:mobile": "NODE_ENV=development webpack --config ./config/webpack.target.mobile.js --watch --display-chunks",
    "postbuild": "yarn shasum",
    "shasum": "shasum -a 256 dist/cozy-bar.min.js && shasum -a 256 dist/cozy-bar.min.css",
    "test": "jest --verbose --coverage",
    "posttest": "bundlesize",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "autoprefixer": "7.2.3",
    "babel-core": "6.26.0",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-runtime": "6.26.0",
    "bundlesize": "0.15.3",
    "cozy-ui": "11.0.0",
    "css-loader": "1.0.0",
    "css-mqpacker": "7.0.0",
    "csswring": "7.0.0",
    "deep-equal": "1.0.1",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.13.1",
    "eslint-config-cozy-app": "0.2.0",
    "file-loader": "2.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "21.3.0-beta.15",
    "json-loader": "0.5.7",
    "mini-css-extract-plugin": "0.4.2",
    "npm-run-all": "4.1.2",
    "postcss": "7.0.2",
    "postcss-css-variables": "0.9.0",
    "postcss-discard-duplicates": "4.0.0",
    "postcss-discard-empty": "4.0.0",
    "postcss-import": "12.0.0",
    "postcss-loader": "3.0.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "semantic-release": "15.9.14",
    "style-loader": "0.19.1",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.2",
    "svg-sprite-loader": "3.9.2",
    "travis-deploy-once": "5.0.7",
    "url-loader": "1.1.1",
    "webpack": "4.19.0",
    "webpack-bundle-analyzer": "3.0.2",
    "webpack-cli": "3.1.0",
    "webpack-merge": "4.1.4",
    "yaml-jest": "1.0.5",
    "yaml-loader": "0.5.0"
  },
  "dependencies": {
    "babel-preset-cozy-app": "^0.3.1",
    "core-js": "^2.5.3",
    "cozy-client-js": "^0.3.19",
    "cozy-device-helper": "^1.4.4",
    "hammerjs": "^2.0.8",
    "lerna-changelog": "^0.7.0",
    "localforage": "^1.5.5",
    "lodash.debounce": "4.0.8",
    "node-polyglot": "^2.2.2",
    "piwik-react-router": "^0.10.0",
    "preact": "^8.2.7",
    "preact-compat": "^3.17.0",
    "react-autosuggest": "^9.3.2",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-persist": "^5.4.0",
    "redux-thunk": "^2.2.0"
  },
  "bundlesize": [
    {
      "path": "./dist/cozy-bar.min.js",
      "maxSize": "102 KB"
    }
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "styl",
      "yaml"
    ],
    "setupFiles": [
      "<rootDir>/test/jestLib/setup.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "transform": {
      "\\.yaml$": "yaml-jest",
      "\\.(js|jsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      "^config/(.*)": "<rootDir>/src/config/$1",
      "\\.(png|gif|jpe?g|svg)$": "<rootDir>/test/__mocks__/fileMock.js",
      "styles": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!cozy-ui)"
    ],
    "globals": {
      "__ALLOW_HTTP__": false,
      "__TARGET__": "browser",
      "__SENTRY_TOKEN__": "token",
      "__DEVELOPMENT__": false,
      "__VERSION__": "4.5.0"
    }
  }
}
